    function initImages() {
        //alert("Start in Images");
        var i;
        for (i = 0; i < ImagePathArry.length; i++) {
            ImageArry[cIndex2] = new Image();
            ImageArry[cIndex2++].src = ImagePathArry[i];
        }

    }
    var ticCount = 0;
    function changeImages() {
        //alert("In Func2");

        var cell = document.getElementById("MainContentImg");
        if (cell) {
            if (counter == ImageArry.length)
                counter = 0;

            if (ticCount <= 0)
                ticCount = ImageTimeArry[counter] / TIME_INT;

            cell.src = ImageArry[counter].src;

            //alert("In Func2 ticCount: " + ticCount + "\r\nCounter: " + counter + "\r\nPath - " + cell.src);
            ticCount--;
            if (ticCount <= 0) {
                counter++;
            }
            setTimeout("changeImages()", (TIME_INT * 1000));
        }
    }
    function changeImages3() {
        //alert("In Func2");

        var cell = document.getElementById("slogan");
        if (counter_top == TopArry.length)
            counter_top = 0;
        cell.src = TopArry[counter_top];
        counter_top++;
        setTimeout("changeImages3()", (TIME_INT_TOP * 1000));

