    function removeHTMLTags(strInputCode) {
        //if(document.getElementById &amp;&amp; document.getElementById("input-code")){
        //	var strInputCode = document.getElementById("input-code").innerHTML;
        /* 
        This line is optional, it replaces escaped brackets with real ones, 
        i.e. < is replaced with < and> is replaced with >
        */
        strInputCode = strInputCode.replace(/&(lt|gt);/g, function (strMatch, p1) {
            return (p1 == "lt") ? "<" : ">";
        });
        var strTagStrippedText = strInputCode.replace(/<\/?[^>]+(>|$)/g, "");
        //alert("Input code:\n" + strInputCode + "\n\nOutput text:\n" + strTagStrippedText);	
        return strTagStrippedText;
        //}	
    }
    function trim(stringToTrim) {
        return stringToTrim.replace(/^\s+|\s+$/g, "");
    }

    function FormatDescriptions() {
        //alert("in FormatDescription");
        var cells = document.getElementsByTagName("td");
        var tempDesc = "";
        var origDesc = "";
        for (var i = 0; i < cells.length; i++) {
            if ((cells[i].name && cells[i].name.substring(0, 19) == "itemCellDescription") ||
             (cells[i].name && cells[i].name.substring(0, 9) == "TempCell_") ||
             (cells[i].name && cells[i].name.substring(0, 14) == "textcontainer1")) {
                origDesc = cells[i].innerHTML;
                //alert(origDesc);
                tempDesc = removeHTMLTags(origDesc);
                var tempDescArry = tempDesc.split(". ");
                if (tempDescArry.length == 1) {
                    tempDesc = origDesc;
                }
                else {
                    tempDesc = "<Font size=2>";
                    for (var k = 0; k < tempDescArry.length; k++) {
                        if (trim(tempDescArry[k]) != "")
                            tempDesc += "â€¢&nbsp;" + trim(tempDescArry[k]) + "<BR>";
                    }
                    tempDesc += "</font>";
                }
                cells[i].innerHTML = tempDesc;
            }
        }
    }
